<?xml version="1.0" encoding="UTF-8"?>
<analysisEngineDescription xmlns="http://uima.apache.org/resourceSpecifier">
  <frameworkImplementation>org.apache.uima.java</frameworkImplementation>
  <primitive>false</primitive>
  <delegateAnalysisEngineSpecifiers>
    <delegateAnalysisEngine key="OrganizationNameFinder">
      <import name="opennlp.fr.OrganizationNameFinder"/>
    </delegateAnalysisEngine>
    <delegateAnalysisEngine key="importTS-AE">
      <import name="common.types.importTS-AE"/>
    </delegateAnalysisEngine>
    <delegateAnalysisEngine key="TokenizerAE">
      <import name="opennlp.fr.TokenizerAE"/>
    </delegateAnalysisEngine>
    <delegateAnalysisEngine key="ChunkerAE">
      <import name="opennlp.fr.ChunkerAE"/>
    </delegateAnalysisEngine>
    <delegateAnalysisEngine key="MphTaggerAE">
      <import name="opennlp.fr.MphTaggerAE"/>
    </delegateAnalysisEngine>
    <delegateAnalysisEngine key="SentenceDetectorAE">
      <import name="opennlp.fr.SentenceDetectorAE"/>
    </delegateAnalysisEngine>
    <delegateAnalysisEngine key="PersonNameFinder">
      <import name="opennlp.fr.PersonNameFinder"/>
    </delegateAnalysisEngine>
    <delegateAnalysisEngine key="PosTaggerAE">
      <import name="opennlp.fr.PosTaggerAE"/>
    </delegateAnalysisEngine>
    <delegateAnalysisEngine key="LocationNameFinder">
      <import name="opennlp.fr.LocationNameFinder"/>
    </delegateAnalysisEngine>
    <delegateAnalysisEngine key="HmmTagger-lemma-fr">
      <import name="HmmTrainerTagger.HmmTagger-lemma-fr"/>
    </delegateAnalysisEngine>
  </delegateAnalysisEngineSpecifiers>
  <analysisEngineMetaData>
    <name>preProcessing-fr-AAE</name>
    <description/>
    <version/>
    <vendor/>
    <configurationParameters searchStrategy="language_fallback">
      <configurationParameter>
        <name>opennlp.uima.SentenceType</name>
        <type>String</type>
        <multiValued>false</multiValued>
        <mandatory>true</mandatory>
        <overrides>
          <parameter>SentenceDetectorAE/opennlp.uima.SentenceType</parameter>
          <parameter>TokenizerAE/opennlp.uima.SentenceType</parameter>
          <parameter>PosTaggerAE/opennlp.uima.SentenceType</parameter>
          <parameter>ChunkerAE/opennlp.uima.SentenceType</parameter>
          <parameter>MphTaggerAE/opennlp.uima.SentenceType</parameter>
          <parameter>PersonNameFinder/opennlp.uima.SentenceType</parameter>
          <parameter>LocationNameFinder/opennlp.uima.SentenceType</parameter>
          <parameter>OrganizationNameFinder/opennlp.uima.SentenceType</parameter>
        </overrides>
      </configurationParameter>
      <configurationParameter>
        <name>opennlp.uima.TokenType</name>
        <type>String</type>
        <multiValued>false</multiValued>
        <mandatory>true</mandatory>
        <overrides>
          <parameter>TokenizerAE/opennlp.uima.TokenType</parameter>
          <parameter>PosTaggerAE/opennlp.uima.TokenType</parameter>
          <parameter>ChunkerAE/opennlp.uima.TokenType</parameter>
          <parameter>MphTaggerAE/opennlp.uima.TokenType</parameter>
          <parameter>PersonNameFinder/opennlp.uima.TokenType</parameter>
          <parameter>LocationNameFinder/opennlp.uima.TokenType</parameter>
          <parameter>OrganizationNameFinder/opennlp.uima.TokenType</parameter>
        </overrides>
      </configurationParameter>
      <configurationParameter>
        <name>opennlp.uima.POSFeature</name>
        <type>String</type>
        <multiValued>false</multiValued>
        <mandatory>true</mandatory>
        <overrides>
          <parameter>PosTaggerAE/opennlp.uima.POSFeature</parameter>
          <parameter>ChunkerAE/opennlp.uima.POSFeature</parameter>
        </overrides>
      </configurationParameter>
      <configurationParameter>
        <name>opennlp.uima.ChunkType</name>
        <type>String</type>
        <multiValued>false</multiValued>
        <mandatory>true</mandatory>
        <overrides>
          <parameter>ChunkerAE/opennlp.uima.ChunkType</parameter>
        </overrides>
      </configurationParameter>
      <configurationParameter>
        <name>opennlp.uima.ChunkTagFeature</name>
        <type>String</type>
        <multiValued>false</multiValued>
        <mandatory>true</mandatory>
        <overrides>
          <parameter>ChunkerAE/opennlp.uima.ChunkTagFeature</parameter>
        </overrides>
      </configurationParameter>
      <configurationParameter>
        <name>SentenceType</name>
        <description>Sentence annotation type which covers token annotations (by default it is set to org.apache.uima.SentenceAnnotation)</description>
        <type>String</type>
        <multiValued>false</multiValued>
        <mandatory>false</mandatory>
        <overrides>
          <parameter>HmmTagger-lemma-fr/SentenceType</parameter>
        </overrides>
      </configurationParameter>
      <configurationParameter>
        <name>TokenFeaturePath</name>
        <description>Token feature path to set. It should be like "type.name:attribute" (set by default to "org.apache.uima.TokenAnnotation:posTag")</description>
        <type>String</type>
        <multiValued>false</multiValued>
        <mandatory>false</mandatory>
        <overrides>
          <parameter>HmmTagger-lemma-fr/TokenFeaturePath</parameter>
        </overrides>
      </configurationParameter>
      <configurationParameter>
        <name>opennlp.uima.POSFeature1</name>
        <type>String</type>
        <multiValued>false</multiValued>
        <mandatory>true</mandatory>
        <overrides>
          <parameter>MphTaggerAE/opennlp.uima.POSFeature</parameter>
        </overrides>
      </configurationParameter>
      <configurationParameter>
        <name>opennlp.uima.NameType</name>
        <type>String</type>
        <multiValued>false</multiValued>
        <mandatory>true</mandatory>
        <overrides>
          <parameter>PersonNameFinder/opennlp.uima.NameType</parameter>
        </overrides>
      </configurationParameter>
      <configurationParameter>
        <name>opennlp.uima.NameType1</name>
        <type>String</type>
        <multiValued>false</multiValued>
        <mandatory>true</mandatory>
        <overrides>
          <parameter>LocationNameFinder/opennlp.uima.NameType</parameter>
        </overrides>
      </configurationParameter>
      <configurationParameter>
        <name>opennlp.uima.NameType2</name>
        <type>String</type>
        <multiValued>false</multiValued>
        <mandatory>true</mandatory>
        <overrides>
          <parameter>OrganizationNameFinder/opennlp.uima.NameType</parameter>
        </overrides>
      </configurationParameter>
    </configurationParameters>
    <configurationParameterSettings>
      <nameValuePair>
        <name>opennlp.uima.ChunkType</name>
        <value>
          <string>common.types.text.Chunk</string>
        </value>
      </nameValuePair>
      <nameValuePair>
        <name>opennlp.uima.TokenType</name>
        <value>
          <string>common.types.text.LexicalUnit</string>
        </value>
      </nameValuePair>
      <nameValuePair>
        <name>opennlp.uima.SentenceType</name>
        <value>
          <string>common.types.text.Sentence</string>
        </value>
      </nameValuePair>
      <nameValuePair>
        <name>opennlp.uima.POSFeature</name>
        <value>
          <string>posTag</string>
        </value>
      </nameValuePair>
      <nameValuePair>
        <name>opennlp.uima.ChunkTagFeature</name>
        <value>
          <string>chunkType</string>
        </value>
      </nameValuePair>
      <nameValuePair>
        <name>SentenceType</name>
        <value>
          <string>common.types.text.Sentence</string>
        </value>
      </nameValuePair>
      <nameValuePair>
        <name>TokenFeaturePath</name>
        <value>
          <string>common.types.text.LexicalUnit:lemma</string>
        </value>
      </nameValuePair>
      <nameValuePair>
        <name>opennlp.uima.POSFeature1</name>
        <value>
          <string>mph</string>
        </value>
      </nameValuePair>
      <nameValuePair>
        <name>opennlp.uima.NameType</name>
        <value>
          <string>common.types.ne.Person</string>
        </value>
      </nameValuePair>
      <nameValuePair>
        <name>opennlp.uima.NameType1</name>
        <value>
          <string>common.types.ne.Location</string>
        </value>
      </nameValuePair>
      <nameValuePair>
        <name>opennlp.uima.NameType2</name>
        <value>
          <string>common.types.ne.Organization</string>
        </value>
      </nameValuePair>
    </configurationParameterSettings>
    <flowConstraints>
      <fixedFlow>
        <node>SentenceDetectorAE</node>
        <node>TokenizerAE</node>
        <node>PosTaggerAE</node>
        <node>MphTaggerAE</node>
        <node>HmmTagger-lemma-fr</node>
        <node>ChunkerAE</node>
        <node>PersonNameFinder</node>
        <node>LocationNameFinder</node>
        <node>OrganizationNameFinder</node>
        <node>importTS-AE</node>
      </fixedFlow>
    </flowConstraints>
    <typePriorities/>
    <fsIndexCollection/>
    <capabilities>
      <capability>
        <inputs/>
        <outputs>
          <type allAnnotatorFeatures="true">common.types.text.Token</type>
          <type allAnnotatorFeatures="true">common.types.text.Sentence</type>
          <type allAnnotatorFeatures="true">common.types.text.LexicalUnit</type>
          <type allAnnotatorFeatures="true">common.types.text.Chunk</type>
        </outputs>
        <languagesSupported>
          <language>fr</language>
        </languagesSupported>
      </capability>
    </capabilities>
    <operationalProperties>
      <modifiesCas>true</modifiesCas>
      <multipleDeploymentAllowed>true</multipleDeploymentAllowed>
      <outputsNewCASes>false</outputsNewCASes>
    </operationalProperties>
  </analysisEngineMetaData>
  <resourceManagerConfiguration>
    <externalResources>
      <externalResource>
        <name>SentenceModel</name>
        <description/>
        <fileResourceSpecifier>
          <fileUrl>file:opennlp/models/fr/fr-sent.bin</fileUrl>
        </fileResourceSpecifier>
        <implementationName>opennlp.uima.sentdetect.SentenceModelResourceImpl</implementationName>
      </externalResource>
      <externalResource>
        <name>TokenModel</name>
        <description/>
        <fileResourceSpecifier>
          <fileUrl>file:opennlp/models/fr/fr-token.bin</fileUrl>
        </fileResourceSpecifier>
        <implementationName>opennlp.uima.tokenize.TokenizerModelResourceImpl</implementationName>
      </externalResource>
      <externalResource>
        <name>PosModel</name>
        <description/>
        <fileResourceSpecifier>
          <fileUrl>file:opennlp/models/fr/fr-pos.bin</fileUrl>
        </fileResourceSpecifier>
        <implementationName>opennlp.uima.postag.POSModelResourceImpl</implementationName>
      </externalResource>
      <externalResource>
        <name>ChunkerModel</name>
        <description/>
        <fileResourceSpecifier>
          <fileUrl>file:opennlp/models/fr/fr-chunk.bin</fileUrl>
        </fileResourceSpecifier>
        <implementationName>opennlp.uima.chunker.ChunkerModelResourceImpl</implementationName>
      </externalResource>
      <externalResource>
        <name>PersonNERModel</name>
        <description/>
        <fileResourceSpecifier>
          <fileUrl>file:opennlp/models/fr/fr-ner-person.bin</fileUrl>
        </fileResourceSpecifier>
        <implementationName>opennlp.uima.namefind.TokenNameFinderModelResourceImpl</implementationName>
      </externalResource>
      <externalResource>
        <name>LocationNERModel</name>
        <description/>
        <fileResourceSpecifier>
          <fileUrl>file:opennlp/models/fr/fr-ner-location.bin</fileUrl>
        </fileResourceSpecifier>
        <implementationName>opennlp.uima.namefind.TokenNameFinderModelResourceImpl</implementationName>
      </externalResource>
      <externalResource>
        <name>OrganizationNERModel</name>
        <description/>
        <fileResourceSpecifier>
          <fileUrl>file:opennlp/models/fr/fr-ner-organization.bin</fileUrl>
        </fileResourceSpecifier>
        <implementationName>opennlp.uima.namefind.TokenNameFinderModelResourceImpl</implementationName>
      </externalResource>
    </externalResources>
    <externalResourceBindings>
      <externalResourceBinding>
        <key>SentenceDetector/opennlp.uima.ModelName</key>
        <resourceName>SentenceModel</resourceName>
      </externalResourceBinding>
      <externalResourceBinding>
        <key>Tokenizer/opennlp.uima.ModelName</key>
        <resourceName>TokenModel</resourceName>
      </externalResourceBinding>
      <externalResourceBinding>
        <key>PosTagger/opennlp.uima.ModelName</key>
        <resourceName>PosModel</resourceName>
      </externalResourceBinding>
      <externalResourceBinding>
        <key>Chunker/opennlp.uima.ModelName</key>
        <resourceName>ChunkerModel</resourceName>
      </externalResourceBinding>
      <externalResourceBinding>
        <key>PersonNameFinder/opennlp.uima.ModelName</key>
        <resourceName>PersonNERModel</resourceName>
      </externalResourceBinding>
      <externalResourceBinding>
        <key>LocationNameFinder/opennlp.uima.ModelName</key>
        <resourceName>LocationNERModel</resourceName>
      </externalResourceBinding>
      <externalResourceBinding>
        <key>OrganizationNameFinder/opennlp.uima.ModelName</key>
        <resourceName>OrganizationNERModel</resourceName>
      </externalResourceBinding>
    </externalResourceBindings>
  </resourceManagerConfiguration>
</analysisEngineDescription>
