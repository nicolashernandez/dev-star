        </fileResourceSpecifier>
        <implementationName>opennlp.uima.namefind.TokenNameFinderModelResourceImpl</implementationName>
      </externalResource>
      <externalResource>
        <name>MoneyModel</name>
        <description/>
        <fileResourceSpecifier>
          <fileUrl>file:models/english/en-ner-money.bin</fileUrl>
        </fileResourceSpecifier>
        <implementationName>opennlp.uima.namefind.TokenNameFinderModelResourceImpl</implementationName>
      </externalResource>
      <externalResource>
        <name>DateModel</name>
        <description/>
        <fileResourceSpecifier>
          <fileUrl>file:models/english/en-ner-date.bin</fileUrl>
        </fileResourceSpecifier>
        <implementationName>opennlp.uima.namefind.TokenNameFinderModelResourceImpl</implementationName>
      </externalResource>
      <externalResource>
        <name>LocationModel</name>
        <description/>
        <fileResourceSpecifier>
          <fileUrl>file:models/english/en-ner-location.bin</fileUrl>
        </fileResourceSpecifier>
        <implementationName>opennlp.uima.namefind.TokenNameFinderModelResourceImpl</implementationName>
      </externalResource>
      <externalResource>
