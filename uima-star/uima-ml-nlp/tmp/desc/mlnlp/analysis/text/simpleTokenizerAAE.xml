<?xml version="1.0" encoding="UTF-8"?><analysisEngineDescription xmlns="http://uima.apache.org/resourceSpecifier">
  <frameworkImplementation>org.apache.uima.java</frameworkImplementation>
  <primitive>false</primitive>
  <delegateAnalysisEngineSpecifiers>
    <delegateAnalysisEngine key="simpleTokenizerAE">
      <import name="mlnlp.analysis.text.simpleTokenizerAE"/>
    </delegateAnalysisEngine>
    <delegateAnalysisEngine key="importTS-AE">
      <import name="common.types.importTS-AE"/>
    </delegateAnalysisEngine>
  </delegateAnalysisEngineSpecifiers>
  <analysisEngineMetaData>
    <name>SimpleTokenizerAAE</name>
    <description/>
    <version>1.0</version>
    <vendor/>
    <configurationParameters searchStrategy="language_fallback">
      <configurationParameter>
        <name>TokenType</name>
        <type>String</type>
        <multiValued>false</multiValued>
        <mandatory>false</mandatory>
        <overrides>
          <parameter>simpleTokenizerAE/TokenType</parameter>
        </overrides>
      </configurationParameter>
      <configurationParameter>
        <name>RegexTokenFilePath</name>
        <type>String</type>
        <multiValued>true</multiValued>
        <mandatory>false</mandatory>
        <overrides>
          <parameter>simpleTokenizerAE/RegexTokenFilePath</parameter>
        </overrides>
      </configurationParameter>
      <configurationParameter>
        <name>CharacterGlueFunctionings</name>
        <type>String</type>
        <multiValued>true</multiValued>
        <mandatory>false</mandatory>
        <overrides>
          <parameter>simpleTokenizerAE/CharacterGlueFunctionings</parameter>
        </overrides>
      </configurationParameter>
      <configurationParameter>
        <name>SentenceSplitting</name>
        <description>command to process the sentence splitting and so to annotate the sentences with the specifed sentence anntotation type</description>
        <type>Boolean</type>
        <multiValued>false</multiValued>
        <mandatory>false</mandatory>
        <overrides>
          <parameter>simpleTokenizerAE/SentenceSplitting</parameter>
        </overrides>
      </configurationParameter>
      <configurationParameter>
        <name>SentenceEndingTokens</name>
        <type>String</type>
        <multiValued>true</multiValued>
        <mandatory>false</mandatory>
        <overrides>
          <parameter>simpleTokenizerAE/SentenceEndingTokens</parameter>
        </overrides>
      </configurationParameter>
      <configurationParameter>
        <name>NonEmptyNewlinesAsSentences</name>
        <type>Boolean</type>
        <multiValued>false</multiValued>
        <mandatory>false</mandatory>
        <overrides>
          <parameter>simpleTokenizerAE/NonEmptyNewlinesAsSentences</parameter>
        </overrides>
      </configurationParameter>
      <configurationParameter>
        <name>SentenceType</name>
        <type>String</type>
        <multiValued>false</multiValued>
        <mandatory>false</mandatory>
        <overrides>
          <parameter>simpleTokenizerAE/SentenceType</parameter>
        </overrides>
      </configurationParameter>
    </configurationParameters>
    <configurationParameterSettings>
      <nameValuePair>
        <name>TokenType</name>
        <value>
          <string>common.types.text.LexicalUnit</string>
        </value>
      </nameValuePair>
      <nameValuePair>
        <name>RegexTokenFilePath</name>
        <value>
          <array>
            <string>simpleTokenizer/tokens-regex-fr.txt</string>
          </array>
        </value>
      </nameValuePair>
      <nameValuePair>
        <name>CharacterGlueFunctionings</name>
        <value>
          <array>
            <string>' 3</string>
            <string>\u2019 3</string>
            <string>- 3</string>
          </array>
        </value>
      </nameValuePair>
      <nameValuePair>
        <name>SentenceSplitting</name>
        <value>
          <boolean>true</boolean>
        </value>
      </nameValuePair>
      <nameValuePair>
        <name>SentenceEndingTokens</name>
        <value>
          <array>
            <string>.</string>
            <string>!</string>
            <string>?</string>
          </array>
        </value>
      </nameValuePair>
      <nameValuePair>
        <name>SentenceType</name>
        <value>
          <string>common.types.text.Sentence</string>
        </value>
      </nameValuePair>
      <nameValuePair>
        <name>NonEmptyNewlinesAsSentences</name>
        <value>
          <boolean>true</boolean>
        </value>
      </nameValuePair>
    </configurationParameterSettings>
    <flowConstraints>
      <fixedFlow>
        <node>simpleTokenizerAE</node>
        <node>importTS-AE</node>
      </fixedFlow>
    </flowConstraints>
    <fsIndexCollection/>
    <capabilities>
      <capability>
        <inputs/>
        <outputs>
          <type allAnnotatorFeatures="true">common.types.text.Token</type>
          <type allAnnotatorFeatures="true">common.types.text.Sentence</type>
        </outputs>
        <languagesSupported/>
      </capability>
    </capabilities>
    <operationalProperties>
      <modifiesCas>true</modifiesCas>
      <multipleDeploymentAllowed>true</multipleDeploymentAllowed>
      <outputsNewCASes>false</outputsNewCASes>
    </operationalProperties>
  </analysisEngineMetaData>
  <resourceManagerConfiguration/>
</analysisEngineDescription>
