  <annotatorImplementationName>example.ae.PreAnnotationMapperAE</annotatorImplementationName>
  <analysisEngineMetaData>
    <name>Pre Annotation Mapper AE</name>
    <description>Illustre un AE produisant certaines annotations en sortie</description>
    <version>1.0</version>
    <vendor/>
    <configurationParameters/>
    <configurationParameterSettings/>
    <typeSystemDescription>
      <imports>
        <import name="example.pre-annotation-mapper_TS"/>
      </imports>
    </typeSystemDescription>
    <typePriorities/>
    <fsIndexCollection/>
