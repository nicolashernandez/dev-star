<rules xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns="http://www.lina.univ-nantes.fr/uima/annotationmapper"
	xsi:schemaLocation="annotationMapperRules.xsd">


	<rule id="P7TSentence2CommonTypes" description="">
		<pattern>
			<patternElement type="common.types.extml.XMLElementAnnotation">
				<constraint>.[@qualifiedName="SENT"]</constraint>
				<create type="common.types.text.Sentence">
				</create>
			</patternElement>
		</pattern>
	</rule>

	<rule id="P7TWord-sw+mw2CommonTypes" description="">
		<pattern>
			<patternElement type="common.types.extml.XMLElementAnnotation">
				<constraint>.[@qualifiedName="w" and (./*[@qualifiedName="sw" and
					@value="true"] or ./*[@qualifiedName="mw" and @value="true"])]
				</constraint>
				<create type="common.types.text.Token">
					<setFeature name="catP" value="./*[@qualifiedName='catP']/@value" />
					<setFeature constraint="./*[@qualifiedName='gender']/@value"
						name="gender" value="./*[@qualifiedName='gender']/@value" />
					<setFeature constraint="./*[@qualifiedName='number']/@value"
						name="number" value="./*[@qualifiedName='number']/@value" />
					<setFeature constraint="./*[@qualifiedName='mood']/@value"
						name="mood" value="./*[@qualifiedName='mood']/@value" />
					<setFeature constraint="./*[@qualifiedName='person']/@value"
						name="person" value="./*[@qualifiedName='person']/@value" />
					<setFeature constraint="./*[@qualifiedName='lemma']/@value"
						name="lemma" value="./*[@qualifiedName='lemma']/@value" />
					<setFeature constraint="./*[@qualifiedName='subcat']/@value"
						name="subCat" value="./*[@qualifiedName='subcat']/@value" />
					<setFeature constraint="./*[@qualifiedName='cat']/@value"
						name="cat" value="./*[@qualifiedName='cat']/@value" />
					<setFeature constraint="./*[@qualifiedName='mph']/@value"
						name="mph" value="./*[@qualifiedName='mph']/@value" />
				</create>
			</patternElement>
		</pattern>
	</rule>

	<rule id="P7TWord-sw+mw-chunk-NP2CommonTypes" description="">
		<pattern>
			<patternElement
				type="common.types.extml.XMLElementAnnotation">
				<constraint>.[@qualifiedName='NP' and (parent[@qualifiedName!='w'])]</constraint> 
				<create type="common.types.text.Chunk">
					<setFeature name="chunkType" value="normalize-space('NP')" />
					<setFeature constraint="./*[@qualifiedName='fct']/@value"
						name="fct" value="./*[@qualifiedName='fct']/@value" />
				</create>
			</patternElement>
		</pattern>
	</rule>
