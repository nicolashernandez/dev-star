<?xml version="1.0" encoding="UTF-8"?><analysisEngineDescription xmlns="http://uima.apache.org/resourceSpecifier">
  <frameworkImplementation>org.apache.uima.java</frameworkImplementation>
  <primitive>false</primitive>
  <delegateAnalysisEngineSpecifiers>
    <delegateAnalysisEngine key="whitespaceTokenizerAAE">
      <import name="mlnlp.analysis.text.whitespaceTokenizerAAE"/>
    </delegateAnalysisEngine>
    <delegateAnalysisEngine key="WSTSPLFileWriterAE">
      <import name="connectors.wstspl.WSTSPLFileWriterAE"/>
    </delegateAnalysisEngine>
  </delegateAnalysisEngineSpecifiers>
  <analysisEngineMetaData>
    <name>wst-CAS2WSTSPL-AAE</name>
    <description/>
    <version>1.0</version>
    <vendor/>
    <configurationParameters searchStrategy="language_fallback">
      <configurationParameter>
        <name>OutputView</name>
        <description>view to create with words and sentences in the WST format. Mandatory.</description>
        <type>String</type>
        <multiValued>false</multiValued>
        <mandatory>true</mandatory>
        <overrides>
          <parameter>WSTSPLFileWriterAE/OutputView</parameter>
        </overrides>
      </configurationParameter>
      <configurationParameter>
        <name>SentenceType</name>
        <description>Type name of the Sentence annotations to turn into sentences separated by a newline character. Optional, by default &lt;code&gt;org.apache.uima.SentenceAnnotation&lt;/code&gt;</description>
        <type>String</type>
        <multiValued>false</multiValued>
        <mandatory>false</mandatory>
        <overrides>
          <parameter>whitespaceTokenizerAAE/SentenceType</parameter>
          <parameter>WSTSPLFileWriterAE/SentenceType</parameter>
        </overrides>
      </configurationParameter>
      <configurationParameter>
        <name>WordType</name>
        <description>Type name of the Word annotations to turn into word separated by a whitespace character. Optional, by default &lt;code&gt;org.apache.uima.TokenAnnotation&lt;/code&gt;</description>
        <type>String</type>
        <multiValued>false</multiValued>
        <mandatory>false</mandatory>
        <overrides>
          <parameter>WSTSPLFileWriterAE/WordType</parameter>
        </overrides>
      </configurationParameter>
      <configurationParameter>
        <name>multiWordJoinCharacter</name>
        <description>By default it is '_'</description>
        <type>String</type>
        <multiValued>false</multiValued>
        <mandatory>false</mandatory>
        <overrides>
          <parameter>WSTSPLFileWriterAE/multiWordJoinCharacter</parameter>
        </overrides>
      </configurationParameter>
      <configurationParameter>
        <name>OutputDirectory</name>
        <description>Name of the directory to write the files from the Context Data Path.</description>
        <type>String</type>
        <multiValued>false</multiValued>
        <mandatory>true</mandatory>
        <overrides>
          <parameter>WSTSPLFileWriterAE/OutputDirectory</parameter>
        </overrides>
      </configurationParameter>
      <configurationParameter>
        <name>TokenType</name>
        <description>Type name of the token annotation to create. Optional, by default org.apache.uima.TokenAnnotation</description>
        <type>String</type>
        <multiValued>false</multiValued>
        <mandatory>false</mandatory>
        <overrides>
          <parameter>whitespaceTokenizerAAE/TokenType</parameter>
        </overrides>
      </configurationParameter>
      <configurationParameter>
        <name>OutputFileSuffix</name>
        <type>String</type>
        <multiValued>false</multiValued>
        <mandatory>false</mandatory>
        <overrides>
          <parameter>WSTSPLFileWriterAE/OutputFileSuffix</parameter>
        </overrides>
      </configurationParameter>
    </configurationParameters>
    <configurationParameterSettings>
      <nameValuePair>
        <name>SentenceType</name>
        <value>
          <string>common.types.text.Sentence</string>
        </value>
      </nameValuePair>
      <nameValuePair>
        <name>WordType</name>
        <value>
          <string>common.types.text.Token</string>
        </value>
      </nameValuePair>
      <nameValuePair>
        <name>OutputView</name>
        <value>
          <string>_WSTSPLView</string>
        </value>
      </nameValuePair>
      <nameValuePair>
        <name>OutputDirectory</name>
        <value>
          <string>/tmp/wstspl</string>
        </value>
      </nameValuePair>
      <nameValuePair>
        <name>TokenType</name>
        <value>
          <string>common.types.text.Token</string>
        </value>
      </nameValuePair>
      <nameValuePair>
        <name>OutputFileSuffix</name>
        <value>
          <string>.txt</string>
        </value>
      </nameValuePair>
    </configurationParameterSettings>
    <flowConstraints>
      <fixedFlow>
        <node>whitespaceTokenizerAAE</node>
        <node>WSTSPLFileWriterAE</node>
      </fixedFlow>
    </flowConstraints>
    <typePriorities>
      <priorityList>
        <type>common.types.text.Sentence</type>
        <type>common.types.text.Token</type>
      </priorityList>
    </typePriorities>
    <fsIndexCollection/>
    <capabilities>
      <capability>
        <inputs/>
        <outputs>
          <type allAnnotatorFeatures="true">common.types.text.Token</type>
          <type allAnnotatorFeatures="true">common.types.text.Sentence</type>
        </outputs>
        <languagesSupported/>
      </capability>
    </capabilities>
    <operationalProperties>
      <modifiesCas>true</modifiesCas>
      <multipleDeploymentAllowed>true</multipleDeploymentAllowed>
      <outputsNewCASes>false</outputsNewCASes>
    </operationalProperties>
  </analysisEngineMetaData>
  <resourceManagerConfiguration/>
</analysisEngineDescription>
