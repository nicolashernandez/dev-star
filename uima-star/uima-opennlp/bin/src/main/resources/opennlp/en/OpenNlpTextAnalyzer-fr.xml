<?xml version="1.0" encoding="UTF-8"?>
<analysisEngineDescription xmlns="http://uima.apache.org/resourceSpecifier">
  <frameworkImplementation>org.apache.uima.java</frameworkImplementation>
  <primitive>false</primitive>
  <delegateAnalysisEngineSpecifiers>
    <delegateAnalysisEngine key="Tokenizer">
      <import name="opennlp.Tokenizer"/>
    </delegateAnalysisEngine>
    <delegateAnalysisEngine key="PosTagger">
      <import name="opennlp.PosTagger"/>
    </delegateAnalysisEngine>
    <delegateAnalysisEngine key="Chunker">
      <import name="opennlp.Chunker"/>
    </delegateAnalysisEngine>
    <delegateAnalysisEngine key="LocationFinder">
      <import name="opennlp.LocationNameFinder"/>
    </delegateAnalysisEngine>
    <delegateAnalysisEngine key="DateFinder">
      <import name="opennlp.DateNameFinder"/>
    </delegateAnalysisEngine>
    <delegateAnalysisEngine key="MoneyFinder">
      <import name="opennlp.MoneyNameFinder"/>
    </delegateAnalysisEngine>
    <delegateAnalysisEngine key="SentenceDetector">
      <import name="opennlp.SentenceDetector"/>
    </delegateAnalysisEngine>
    <delegateAnalysisEngine key="PercentageFinder">
      <import name="opennlp.PercentageNameFinder"/>
    </delegateAnalysisEngine>
    <delegateAnalysisEngine key="TimeFinder">
      <import name="opennlp.TimeNameFinder"/>
    </delegateAnalysisEngine>
    <delegateAnalysisEngine key="PersonFinder">
      <import name="opennlp.PersonNameFinder"/>
    </delegateAnalysisEngine>
    <delegateAnalysisEngine key="OrganizationFinder">
      <import name="opennlp.OrganizationNameFinder"/>
    </delegateAnalysisEngine>
  </delegateAnalysisEngineSpecifiers>
  <analysisEngineMetaData>
    <name>OpenNlpTextAnalyzer</name>
    <description/>
    <version>1.4.4</version>
    <vendor>OpenNlp</vendor>
    <configurationParameters searchStrategy="language_fallback"/>
    <configurationParameterSettings/>
    <flowConstraints>
      <fixedFlow>
        <node>SentenceDetector</node>
        <node>Tokenizer</node>
        <node>PosTagger</node>
        <node>Chunker</node>
      </fixedFlow>
    </flowConstraints>
    <typePriorities/>
    <fsIndexCollection/>
    <capabilities>
      <capability>
        <inputs/>
        <outputs>
          <type allAnnotatorFeatures="true">opennlp.uima.Token</type>
          <type allAnnotatorFeatures="true">opennlp.uima.Time</type>
          <type allAnnotatorFeatures="true">opennlp.uima.Sentence</type>
          <type allAnnotatorFeatures="true">opennlp.uima.Person</type>
          <type allAnnotatorFeatures="true">opennlp.uima.Percentage</type>
          <type allAnnotatorFeatures="true">opennlp.uima.Organization</type>
          <type allAnnotatorFeatures="true">opennlp.uima.Money</type>
          <type allAnnotatorFeatures="true">opennlp.uima.Location</type>
          <type allAnnotatorFeatures="true">opennlp.uima.Date</type>
          <type allAnnotatorFeatures="true">opennlp.uima.Chunk</type>
        </outputs>
        <languagesSupported>
          <language>fr</language>
        </languagesSupported>
      </capability>
    </capabilities>
    <operationalProperties>
      <modifiesCas>true</modifiesCas>
      <multipleDeploymentAllowed>false</multipleDeploymentAllowed>
      <outputsNewCASes>false</outputsNewCASes>
    </operationalProperties>
  </analysisEngineMetaData>
  <resourceManagerConfiguration>
    <externalResources>
      <externalResource>
        <name>SentenceModel</name>
        <description/>
        <fileResourceSpecifier>
          <fileUrl>file:models/french/fr-sent/fr-sent.bin</fileUrl>
        </fileResourceSpecifier>
        <implementationName>opennlp.uima.sentdetect.SentenceModelResourceImpl</implementationName>
      </externalResource>
      <externalResource>
        <name>TokenModel</name>
        <description/>
        <fileResourceSpecifier>
          <fileUrl>file:models/french/fr-token/fr-token.bin</fileUrl>
        </fileResourceSpecifier>
        <implementationName>opennlp.uima.tokenize.TokenizerModelResourceImpl</implementationName>
      </externalResource>
      <externalResource>
        <name>PersonModel</name>
        <description/>
        <fileResourceSpecifier>
          <fileUrl>file:models/english/en-ner-person.bin</fileUrl>
        </fileResourceSpecifier>
        <implementationName>opennlp.uima.namefind.TokenNameFinderModelResourceImpl</implementationName>
      </externalResource>
      <externalResource>
        <name>PercentageModel</name>
        <description/>
